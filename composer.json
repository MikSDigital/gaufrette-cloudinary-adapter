{
  "name": "scc/gaufrette-cloudinary-adapter",
  "description": "cloudinary adapter for Gaufrette",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "support": {
    "source": "https://github.com/scc-digital/gaufrette-cloudinary-adapter",
    "issues": "https://github.com/scc-digital/gaufrette-cloudinary-adapter/issues"
  },
  "require": {
    "php": "^7.0",
    "knplabs/gaufrette": "^0.5.0",
    "cloudinary/cloudinary_php": "^1.8",
    "monolog/monolog": "~1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Scc\\Gaufrette\\Cloudinary\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scc\\Tests\\Gaufrette\\Cloudinary\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
